If "<RTK$Dir>"="" Then Set RTK$Dir <Obey$Dir>
IfThere <RTK$Dir>.html.index/html Then Set RTK$Help <RTK$Dir>.html.index/html
If ("<Alias$@RunType_FAF>"="") OR ("<RTK$Help>"="") Then Filer_Run <RTK$Dir>.README Else Filer_Run <RTK$Help>
